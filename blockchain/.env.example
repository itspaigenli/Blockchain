// Configuration variables for Hela Testnet deployment
// Copy this file to .env and fill in your values

# Your private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Hela Testnet RPC URL (default: https://testnet-rpc.helachain.com)
HELA_TESTNET_RPC_URL=https://testnet-rpc.helachain.com

# Optional: Sepolia testnet configuration
SEPOLIA_RPC_URL=
SEPOLIA_PRIVATE_KEY=
